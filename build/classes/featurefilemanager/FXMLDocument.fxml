            <TreeView fx:id="featureDirectory" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <HBox BorderPane.alignment="CENTER">
               <children>
                  <TextField prefHeight="31.0" prefWidth="270.0" promptText="Search Feature" />
                  <Button fx:id="searchButton" mnemonicParsing="false" prefWidth="30.0" />
